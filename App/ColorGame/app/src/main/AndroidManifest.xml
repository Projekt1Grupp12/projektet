<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"    package="com.example.anna.colorgame">    <uses-permission android:name="android.permission.INTERNET" />    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />    <application        android:allowBackup="true"        android:icon="@mipmap/ic_launcher"        android:label="@string/app_name"        android:roundIcon="@mipmap/ic_launcher_round"        android:supportsRtl="true"        android:theme="@style/AppTheme">        <activity            android:name=".MainFrame"            android:screenOrientation="portrait">            <intent-filter>                <action android:name="android.intent.action.MAIN" />                <category android:name="android.intent.category.LAUNCHER" />            </intent-filter>        </activity>        <activity            android:name=".MainMenu"            android:parentActivityName=".MainFrame"            android:screenOrientation="portrait">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>            <meta-data                android:name="android.support.PARENT_ACTIVITY"                android:value=".MainFrame" />        </activity>        <activity            android:name=".ChooseGame"            android:parentActivityName=".MainMenu"            android:screenOrientation="portrait">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>            <meta-data                android:name="android.support.PARENT_ACTIVITY"                android:value=".MainMenu" />        </activity>        <activity            android:name=".PuzzleGame"            android:screenOrientation="portrait"            android:noHistory="true">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name=".TrafficGame"            android:screenOrientation="portrait">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name=".DuelGame"            android:screenOrientation="portrait">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>        </activity>        <activity            android:name=".HighScore"            android:screenOrientation="portrait"            android:parentActivityName=".MainMenu"            android:noHistory="true">            <intent-filter>                <category android:name="android.intent.category.DEFAULT" />            </intent-filter>            <meta-data                android:name="android.support.PARENT_ACTIVITY"                android:value=".MainMenu" />        </activity>    </application></manifest>